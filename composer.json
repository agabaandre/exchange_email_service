{
    "name": "agabaandre-office365/exchange-email-service",
    "description": "General-purpose email service using Microsoft Graph API with OAuth 2.0 authentication. Requires https://graph.microsoft.com/Mail.Send permission.",
    "type": "library",
    "version": "1.1.2",
    "keywords": [
        "email",
        "microsoft",
        "graph",
        "oauth",
        "laravel",
        "laravel11",
        "laravel12",
        "exchange",
        "office365"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Andre Agaba",
            "email": "agabaandre@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "vlucas/phpdotenv": "^5.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/container": "^8.0|^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "AgabaandreOffice365\\ExchangeEmailService\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AgabaandreOffice365\\ExchangeEmailService\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AgabaandreOffice365\\ExchangeEmailService\\ExchangeEmailServiceProvider"
            ],
            "aliases": {
                "ExchangeEmail": "AgabaandreOffice365\\ExchangeEmailService\\ExchangeEmailService"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "homepage": "https://github.com/agabaandre/exchange_email_service",
    "support": {
        "issues": "https://github.com/agabaandre/exchange_email_service/issues",
        "source": "https://github.com/agabaandre/exchange_email_service"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/agabaandre"
        }
    ]
}
